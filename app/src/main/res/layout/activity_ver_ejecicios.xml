<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dtxt_verEjercicios_nombre"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VerEjeciciosActivity">

    <TextView
        android:id="@+id/txt_verEjercicios_nombreEjercicio"
        android:layout_width="98dp"
        android:layout_height="24dp"
        android:layout_marginStart="156dp"
        android:layout_marginLeft="156dp"
        android:layout_marginTop="106dp"
        android:layout_marginEnd="157dp"
        android:layout_marginRight="157dp"
        android:layout_marginBottom="31dp"
        app:layout_constraintBottom_toTopOf="@+id/img_verEjercicios_imagen"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_verEjercicios_imagen"
        android:layout_width="323dp"
        android:layout_height="291dp"
        android:layout_marginBottom="312dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_verEjercicios_nombreEjercicio"
        app:layout_constraintVertical_bias="0.939"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/txt_verEjercicios_clasificacionEjercicio"
        android:layout_width="85dp"
        android:layout_height="24dp"
        android:layout_marginBottom="256dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.481"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_verEjercicios_imagen"
        app:layout_constraintVertical_bias="1.0" />
</androidx.constraintlayout.widget.ConstraintLayout>